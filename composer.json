{
  "name": "approached/laravel-date-international",
  "description": "Date manipulator for laravel",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "date",
    "manipulator",
    "international",
    "intl",
    "laravel"
  ],
  "homepage": "https://github.com/approached/laravel-date-international",
  "authors": [
    {
      "name": "Alexej Kloos",
      "email": "alexejkloos@gmail.com",
      "homepage": "http://alcodo.com",
      "role": "Developer"
    }
  ],
  "require": {
    "laravel/framework": "~5.0",
    "nesbot/carbon": "^1.0|^2.0",
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Approached\\LaravelDateInternational\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "suggest": {
    "ext-intl": "to use the component with locales other than \"en\""
  }
}
